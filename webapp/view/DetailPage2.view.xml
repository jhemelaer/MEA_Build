			<List mode="SingleSelectMaster" width="auto" headerText="" footerText="" backgroundDesign="Solid" showSeparators="All" growing="false"
				growingThreshold="20" growingScrollToLoad="true" visible="true" class="sapUiTinyMargin sapUiNoContentPadding">
				<infoToolbar/>
				<headerToolbar/>
				<items>
					<CustomListItem type="Active" selected="false" visible="true">
						<content>
							<HBox alignItems="Center" direction="Row" fitContainer="false" width="auto" height="auto" justifyContent="SpaceAround" renderType="Div"
								visible="true" displayInline="false">
								<items>
									<f:Avatar src="sap-icon://employee-rejections" initials="" displayShape="Circle" displaySize="XS" imageFitType="Contain"/>
